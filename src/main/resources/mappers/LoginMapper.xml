<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="LoginMapper">
    <select id="getAdminLogin" parameterType="com.jj.jejuTour.Login.vo.LoginVo">
            SELECT
                COUNT(create_dt)
            FROM
                admin_info
            WHERE admin_id = #{id} AND admin_pwd = #{admin_pwd}
    </select>
</mapper>